                                                url_list  = agregarParametroAURL(url_list,'bMo',$('input#bMo').val());
                                                url_pager = agregarParametroAURL(url_pager,'bMo',$('input#bMo').val());
                                            $.post(url_list, function(data) { $('.table tbody').html(data); miExpander(); });
                                            $.post(url_pager, function(data) { $('.miPager').html(data); });
